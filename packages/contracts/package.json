{
  "name": "contracts",
  "license": "MIT",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "scripts": {
    "----- EXTERNAL -----": "---------------------------",
    "prepare": "yarn build && mud codegen-libdeploy --out src/test",
    "devnode": "mud devnode",
    "test": "mud test",
    "build": "mud types && node build-deploy.js -c game-config.json",
    "dev": "node build-deploy.js -c game-config.json && mud deploy-contracts --dev --watch --openUrl http://localhost:3000",
    "deploy": "mud deploy-contracts --gasPrice 1000000 --deployerPrivateKey 0x26e86e45f6fc45ec6e2ecd128cec80fa1d1505e5507dcd2ae58c3130a7a97b48 --rpc https://follower.testnet-chain.linfra.xyz/",
    "deploy:prod": "mud deploy --deployerPrivateKey 0x26e86e45f6fc45ec6e2ecd128cec80fa1d1505e5507dcd2ae58c3130a7a97b48 --chainSpec ./lattice-chain-spec.json --deployClient --netlifySlug mud --netlifyPersonalToken q49-wyrS2OGAMk60KsCyfyQGD0QgCcjfqKmtiu8RtQc",
    "lint": "yarn prettier && yarn solhint",
    "server": "tsc && node src/server/index.js",
    "----- INTERNAL -----": "---------------------------",
    "prettier": "prettier --write 'src/**/*.sol'",
    "solhint": "solhint --config ./.solhint.json 'src/**/*.sol' --fix",
    "build-deploy": "./src/scripts/build-deploy.js"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/bytes": "^5.7.0",
    "@ethersproject/providers": "^5.7.0",
    "@latticexyz/cli": "1.39.0",
    "@latticexyz/noise": "^1.40.0",
    "@latticexyz/solecs": "1.40.0",
    "@latticexyz/std-contracts": "1.40.0",
    "@latticexyz/utils": "1.40.0",
    "@manifoldxyz/royalty-registry-solidity": "https://github.com/manifoldxyz/royalty-registry-solidity.git#c5ad6269d37e180fbf449a1f5ba2d2ff6c441efc",
    "@opengsn/contracts": "^2.2.6",
    "@rari-capital/solmate": "https://github.com/rari-capital/solmate.git#851ea3baa4327f453da723df75b1093b58b964dc",
    "@solidstate/contracts": "^0.0.52",
    "@typechain/ethers-v5": "^9.0.0",
    "@types/express": "^4.17.17",
    "@types/glob": "^7.2.0",
    "@types/node": "^17.0.21",
    "abdk-libraries-solidity": "^3.1.0",
    "base64-sol": "https://github.com/Brechtpd/base64.git#4d85607b18d981acff392d2e99ba654305552a97",
    "copyfiles": "^2.4.1",
    "ejs": "^3.1.8",
    "ethers": "^5.7.1",
    "forge-std": "https://github.com/foundry-rs/forge-std.git#b4f121555729b3afb3c5ffccb62ff4b6e2818fd3",
    "glob": "^8.0.3",
    "hardhat": "https://gitpkg.now.sh/latticexyz/hardhat/packages/hardhat-core?build",
    "husky": ">=8.0.2",
    "memmove": "https://github.com/brockelmore/memmove.git#d577ecd1bc43656f4032edf4daa9797f756a8ad2",
    "npm-run-all": "^4.1.5",
    "openzeppelin-solidity": "https://github.com/OpenZeppelin/openzeppelin-contracts.git#57725120581e27ec469e1c7e497a4008aafff818",
    "prettier": "^2.6.2",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "rimraf": "^3.0.2",
    "run-pty": "^3.0.0",
    "solhint": "^3.3.7",
    "solidity-trigonometry": "https://github.com/mds1/solidity-trigonometry.git",
    "ts-node": "^10.7.0",
    "typechain": "8.1.1",
    "typescript": "^4.6.2",
    "wait-on": "^6.0.1"
  },
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true
  }
}
